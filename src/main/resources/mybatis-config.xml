<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
	<settings>
		<!-- null 값이 발생하는 경우 빈칸이 아닌 null로 처리 -->
		<setting name = "jdbcTypeForNull" value="NULL" />
	</settings>
	<typeAliases>
		<typeAlias type="kr.or.eum.community.model.vo.Community" alias="comm"/>
		<typeAlias type="kr.or.eum.member.model.vo.Member" alias="member"/>
		<typeAlias type="kr.or.eum.product.model.vo.Product" alias="pro"/>
		<typeAlias type="kr.or.eum.member.model.vo.Member" alias="m"/>
		<typeAlias type="kr.or.eum.manager.model.vo.Question" alias="q"/>
	</typeAliases>
	<typeHandlers>
        <typeHandler handler="kr.or.eum.LongHandler" javaType="String" jdbcType="LONGVARCHAR"/>
    </typeHandlers>

</configuration>
